{
  "name": "famous-seed",
  "version": "0.1.0",
  "description": "seed for a browserified version of famous",
  "browser": "src/index.js",
  "scripts": {
    "build-main": "browserify src/main.js -t [ babelify --playground --experimental ] | uglifyjs > public/main.bundle.js",
    "build-worker": "browserify src/worker.js -t [ babelify --playground --experimental ]| uglifyjs > public/main.bundle.js",
    "watch-main": "watchify src/main.js -o public/main.bundle.js -d -v -t [ babelify --playground --experimental ]",
    "watch-worker": "watchify src/worker.js -o public/worker.bundle.js -d -v -t [ babelify --playground --experimental ]",
    "start": "npm run build && serve public/ -p 1337",
    "start-dev": "npm run watch & serve public/ -p 1337",
    "test": "npm run lint -s && npm run build",
    "build": "npm run build-main & npm run build-worker",
    "watch": "npm run watch-main & npm run watch-worker",
    "lint-eslint": "eslint src/",
    "lint-jscs": "jscs src/",
    "lint": "npm run lint-eslint && npm run lint-jscs"
  },
  "devDependencies": {
    "babelify": "^5.0.3",
    "browser-sync": "^2.0.0-rc10",
    "browserify": "^8.0.3",
    "eslint": "^0.12.0",
    "jscs": "^1.7.3",
    "serve": "^1.4.0",
    "uglify-js": "^2.4.15",
    "watchify": "^2.1.0"
  },
  "author": "TheAlphaNerd",
  "license": "ISC",
  "dependencies": {
    "brfs": "^1.1.1",
    "famous-components": "git+ssh://git@github.famo.us/platform/components#master",
    "famous-core": "git+ssh://git@github.famo.us/platform/core#master",
    "famous-dom-renderers": "git+ssh://git@github.famo.us/platform/dom-renderers#master",
    "famous-engine": "git+ssh://git@github.famo.us/platform/engine#master",
    "famous-layouts": "git+ssh://git@github.famo.us/platform/layouts#master",
    "famous-math": "git+ssh://git@github.famo.us/platform/math#master",
    "famous-physics": "git+ssh://git@github.famo.us/platform/physics#master",
    "famous-renderers": "git+ssh://git@github.famo.us/platform/renderers#master",
    "famous-dom-renderables": "git+ssh://git@github.famo.us/platform/dom-renderables#master",
    "famous-stylesheets": "git+ssh://git@github.famo.us/platform/stylesheets#master",
    "famous-router": "git+ssh://git@github.famo.us/platform/router#master",
    "famous-stylesheets": "git+ssh://git@github.famo.us/platform/stylesheets#master",
    "famous-transitions": "git+ssh://git@github.famo.us/platform/transitions#master",
    "famous-utilities": "git+ssh://git@github.famo.us/platform/utilities#master",
    "famous-webgl-renderables": "git+ssh://git@github.famo.us/platform/webgl-renderables#master",
    "famous-webgl-renderers": "git+ssh://git@github.famo.us/platform/webgl-renderers#master",
    "famous-webgl-geometries": "git+ssh://git@github.famo.us/platform/webgl-geometries#master",
    "famous-webgl-materials": "git+ssh://git@github.famo.us/platform/webgl-materials#master",
    "famous-handlers": "git+ssh://git@github.famo.us/platform/handlers#master",
    "famous-webgl-shaders": "git+ssh://git@github.famo.us/platform/webgl-shaders#master"
  }
}
